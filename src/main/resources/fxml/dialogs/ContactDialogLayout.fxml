<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="285.0" prefWidth="686.0"
            xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="de.outlookklon.fxcontroller.dialogs.ContactDialogController">
    <center>
        <GridPane hgap="10" BorderPane.alignment="CENTER">
            <padding>
                <Insets bottom="10" left="10" right="10" top="10" />
            </padding>
            <columnConstraints>
                <ColumnConstraints fillWidth="true" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints fillWidth="true" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="10.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="10.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="10.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="10.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="10.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="10.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <Label fx:id="firstNameLabel" text="First name" GridPane.halignment="RIGHT" />
                <Label fx:id="lastNameLabel" text="Last name" GridPane.halignment="RIGHT" GridPane.rowIndex="1" />
                <Label fx:id="displayNameLabel" text="Display name" GridPane.halignment="RIGHT" GridPane.rowIndex="2" />
                <Label fx:id="nickNameLabel" text="Nickname" GridPane.halignment="RIGHT" GridPane.rowIndex="3" />
                <Label fx:id="mailLabel" text="Mail address" GridPane.halignment="RIGHT" GridPane.rowIndex="4" />
                <Label fx:id="mail2Label" text="Second mail address" GridPane.halignment="RIGHT" GridPane.rowIndex="5" />
                <Label fx:id="dutyPhoneLabel" text="Duty phone" GridPane.columnIndex="2" GridPane.halignment="RIGHT" />
                <Label fx:id="privatePhoneLabel" text="Private phone" GridPane.columnIndex="2" GridPane.halignment="RIGHT" GridPane.rowIndex="1" />
                <Label fx:id="mobilePhoneLabel" text="Mobile phone" GridPane.columnIndex="2" GridPane.halignment="RIGHT" GridPane.rowIndex="2" />
                <TextField fx:id="firstNameText" GridPane.columnIndex="1" />
                <TextField fx:id="lastNameText" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                <TextField fx:id="displayNameText" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                <TextField fx:id="nickNameText" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                <TextField fx:id="mailText" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                <TextField fx:id="mail2Text" GridPane.columnIndex="1" GridPane.rowIndex="5" />
                <TextField fx:id="dutyPhoneText" GridPane.columnIndex="3" />
                <TextField fx:id="privatePhoneText" GridPane.columnIndex="3" GridPane.rowIndex="1" />
                <TextField fx:id="mobilePhoneText" GridPane.columnIndex="3" GridPane.rowIndex="2" />
            </children>
        </GridPane>
    </center>
    <bottom>
        <HBox prefHeight="37.0" prefWidth="686.0" BorderPane.alignment="CENTER" spacing="10">
            <padding>
                <Insets bottom="10" left="10" right="10" />
            </padding>
            <children>
                <Button fx:id="ok" mnemonicParsing="false" prefHeight="25.0" prefWidth="85.0" onAction="#onOk" text="OK" />
                <Button fx:id="abort" mnemonicParsing="false" prefHeight="25.0" prefWidth="85.0" onAction="#onAbort" text="Abort" />
            </children>
        </HBox>
    </bottom>
</BorderPane>
